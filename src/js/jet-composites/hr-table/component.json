{
  "name": "hr-table",
  "version": "1.0.0",
  "jetVersion": "^8.1.0",
  "properties": {
    "componentId": {
      "type": "string"
    },
    "tableProperties": {
      "type": "object"
    },
    "pagingProperties": {
      "type": "object"
    },
    "modelProperties": {
      "type": "object"
    },
    "templateProperties": {
      "type": "object"
    }
  },
  "methods": {
    "getDataProvider": {
      "return": "object"
    },
    "refreshDatasource": {
      "return": "void"
    }
  }, 
  "events": {
    "rowSelectionChange": {
      "description": "Event raised when a table row is selected",
      "bubbles" : true,
      "cancelable" : false,
      "detail" : {
        "rowIndex": { "type" : "number" },
        "rowKey": { "type" : "number" }
      }
    }
  }, 
  "slots": {

  }
}